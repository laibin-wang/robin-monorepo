{
	"name": "@robin/monitor-sdk",
	"version": "0.1.0",
	"description": "多框架通用的前端监控与日志埋点 SDK",
	"keywords": [
		"analytics",
		"error-tracking",
		"logging",
		"monitor",
		"performance"
	],
	"license": "MIT",
