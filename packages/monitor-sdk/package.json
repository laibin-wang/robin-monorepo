{
    "name": "@robin/monitor-sdk",
    "version": "0.1.0",
    "type": "module",
    "description": "多框架通用的前端监控与日志埋点 SDK",
    "keywords": [
        "monitor",
        "error-tracking",
        "analytics",
        "performance",
        "logging"
    ],
    "author": "cotc",
    "license": "MIT",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.esm.js",
